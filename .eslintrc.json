{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "prettier",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:jsdoc/recommended"
    ],
    "plugins": ["prettier", "jsdoc"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "warn"
    }
}
